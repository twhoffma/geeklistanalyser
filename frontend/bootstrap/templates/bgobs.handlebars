  {{#if tobsies.length}}
  	<li class="lists">
      <div class="tobsies">
      	<span><i class="fa fa-handshake-o"></i></span>
      	{{#each tobies}}
        	<span class="tp obs"><a href="https://www.boardgamegeek.com/geeklist/item/{{this}}" target="_blank">{{this}}</a></span>
        {{/each}}
      </div>
  	</li>
  {{/if}}
  {{#if obsies.length}}
  	<li class="lists">
      <div class="obsies">
      	<span><i class="fas fa-list"></i></span>
      	{{#each obsies}}
        	<span class="tp obs"><a href="https://www.boardgamegeek.com/geeklist/item/{{this}}" target="_blank">{{this}}</a></span>
        {{/each}}
      </div>
  	</li>
  {{/if}}
  {{#if probsies.length}}
  	<li class="lists">
      <div class="obsies">
      	<span><i class="far fa-list-alt"></i></span>
      	{{#each probsies}}
        	<span class="tp obs"><a href="https://www.boardgamegeek.com/previews/{{previewid}}/item/{{objectid}}" target="_blank">{{objectid}}</a></span>
        {{/each}}
      </div>
  	</li>
  {{/if}}
